<% if @employee.errors.any? %>
<div>
  <h3>
   <%= pluralize(@employee.errors.count, "error") %>
   prohibited this from being saved
  </h3>
    <ul>
        <% @employee.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
        <% end %>
    </ul>
</div>
<% end %>


<%= form_for(@employee) do |f| %>
<%= f.label :first_name, "First Name" %>
<%= f.text_field :first_name %>
<%= f.label :last_name, "Last Name" %>
<%= f.text_field :last_name %>
<%= f.label :alias, "Alias" %>
<%= f.text_field :alias %>
<%= f.label :title, "Title" %>
<%= f.text_field :title %>
<%= f.label :office, "Office" %>
<%= f.text_field :office %>
<%= f.label :dog_id, "Dog" %>
<%= f.collection_select :dog_id, Dog.all, :id, :name %>
<%= f.submit %>
<% end %>